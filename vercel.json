{
    "outputDirectory": "dist",
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "devCommand": "npm run start",
    "rewrites": [
        { "handle": "filesystem" },
        { "source": "pdocs/(.*)", "destination": "/$1" },
        { "source": "pdocs", "destination": "/" }
    ]
}